module Region exposing (all, idString, name, Region, RegionId(..))

import Dict


type alias Region =
    { id : RegionId
    , name : String
    }


type RegionId
    = RegionId String


name : RegionId -> Maybe String
name id =
    let
        d =
            Dict.fromList allData

        r =
            Region id ""
    in
        Dict.get (idString r) d


idString : Region -> String
idString { id } =
    let
        (RegionId x) =
            id
    in
        x


all : List Region
all =
    List.map fromPair allData |> List.sortBy .name


fromPair : ( String, String ) -> Region
fromPair ( id, name ) =
    Region (RegionId id) name


